{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Swhere",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "",
      "program": "${workspaceFolder}/Source/out/msbuild/bin/Swhere/Debug/net6.0/Swhere.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "Debug Soup",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/out/soup",
      "args": [
        "restore",
        "Source/Client/CLI"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          // "text": "catch throw",
          "ignoreFailures": true
        }
      ]
    }
  ]
}